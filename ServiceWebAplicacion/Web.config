<configuration>
    <system.web>
      <compilation debug="true" targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" />
        <identity impersonate="false" />
    </system.web>
    <system.webServer>
        <staticContent>
            <mimeMap fileExtension=".aspx" mimeType="application/aspx" />
            <mimeMap fileExtension=".asmx" mimeType="text/xml" />
        </staticContent>
    </system.webServer>

</configuration>
